{
  "name": "cubicler",
  "version": "2.6.0",
  "description": "Modular AI Orchestration Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest --config vitest.config.ts",
    "test:run": "vitest --run --config vitest.config.ts",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "run-integration": "vitest --run --config vitest.integration.config.ts",
    "test:integration": "npm run run-integration",
    "test:integration:shell": "tests/integration/run-integration-tests.sh",
    "build": "tsup",
    "build:tsc": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && node dist/index.js",
    "dev:watch": "npm run build && node --watch dist/index.js",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t cubicler .",
    "docker:run": "docker run -p 1503:1503 --env-file .env cubicler",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "docker:example": "docker-compose -f docker-compose.example.yml up --build",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build-hub": "docker build -t cubicler/cubicler:latest -t cubicler/cubicler:2.6.0 .",
    "docker:push-hub": "docker push cubicler/cubicler:latest && docker push cubicler/cubicler:2.6.0",
    "docker:push": "docker push cubicler/cubicler:latest && docker push cubicler/cubicler:2.6.0",
    "docker:publish": "npm run docker:build-hub && npm run docker:push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hainayanda/Cubicler.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/hainayanda/Cubicler/issues"
  },
  "homepage": "https://github.com/hainayanda/Cubicler#readme",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "mock-fs": "^5.5.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@cubicler/cubicagent-openai": "^2.6.4",
    "axios": "^1.11.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.8.0"
  }
}
