# Example environment variables for Cubicler configuration

# Port for the Cubicler server
CUBICLER_PORT=1503

# Enable CORS headers (set to 'true' to enable, leave unset or 'false' to disable)
# ENABLE_CORS=true

# ===== ORCHESTRATION MIDDLEWARE CONFIGURATION =====

# Where Cubicler will get the list of available providers
# Can be a local file or remote URL (JSON format)
CUBICLER_PROVIDERS_LIST=providers.example.json

# Where Cubicler will get the list of available AI agents  
# Can be a local file or remote URL (JSON format)
CUBICLER_AGENTS_LIST=agents.example.json

# ===== CACHE CONFIGURATION =====

# Providers configuration caching
PROVIDERS_CACHE_ENABLED=true
PROVIDERS_CACHE_TIMEOUT=600  # 10 minutes in seconds

# Agents configuration caching
AGENTS_CACHE_ENABLED=true
AGENTS_CACHE_TIMEOUT=600  # 10 minutes in seconds

# ===== TIMEOUT CONFIGURATION =====

# Configuration loading timeout (in milliseconds)
CUBICLER_CONFIG_TIMEOUT=10000

# Default timeout for all HTTP requests (in milliseconds)
DEFAULT_CALL_TIMEOUT=30000

# Timeout for provider function calls (optional)
# PROVIDER_CALL_TIMEOUT=30000

# Timeout for agent calls (optional)
# AGENT_CALL_TIMEOUT=90000
# ANOTHER_SERVICE_URL=https://api.example.com